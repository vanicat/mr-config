[Bibliothèque calibre]
checkout = git clone "moi@corbeau:Bibliothèque calibre"
         git remote add "moi@gobelin:Bibliothèque calibre"
         git annex init;
         git annex sync;
         git annex get
update = test ! -e ~/.calibre_calibre\ GUI.lock
       git annex sync
       git annex get
push = test ! -e ~/.calibre_calibre\ GUI.lock
     git annex add .
     git annex sync
fullsync = test ! -e ~/.calibre_calibre\ GUI.lock
         git annex add .; git annex sync; git annex get
